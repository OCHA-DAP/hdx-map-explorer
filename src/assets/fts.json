      "operations": [
        {
          "type": "sort",
          "options": {
            "columns": "#date+end",
            "reverse": "on"
          }
        },
        {
          "type": "deduplicate",
          "options": {
          "columns": "#country+name"
          }
        }
      ]
    }
  ]
